.dialog {
    border-radius: 4px;
    -webkit-border-radius: 4px;
    -moz-border-radius: 4px;
    background: #FFFFFF;
    position: fixed;
    z-index: 1000;
    top: 10em;
    left: 10em;
    display: none;
    padding: 0 25px;
    -webkit-box-shadow: 0 0 16px #000000;
    -moz-box-shadow: 0 0 16px #000000;
    box-shadow: 0 0 16px #000000
}

.dialog.show {
    display: block
}

.dialog .dialog_close {
    height: 50px;
    width: 50px;
    float: right;
    cursor: pointer;
    background: url(../images/dialog_close.png) no-repeat center center;
    margin-right: -15px
}

.dialog .dialog_title {
    height: 50px;
    line-height: 50px;
